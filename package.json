{
  "name": "assessment",
  "version": "1.0.0",
  "description": "Assessment App",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config build-utils/webpack.config.js --env env=dev",
    "lint": "eslint src",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write \"src/**/*.+(js|jsx|json|css|md)\"",
    "build": "webpack --config build-utils/webpack.config.js --env env=prod",
    "dev": "webpack --mode development",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "gitpush": "git push && git push https://github.com/RadomirRalev/react-upskill-projects"
  },
  "homepage": "http://RadomirRalev.github.io/react-upskill-projects",
  "author": "Radomir Ralev",
  "license": "MIT",
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.3.0",
    "css-loader": "^5.0.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^4.5.0",
    "prettier": "2.1.2",
    "react-hot-loader": "^4.13.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.4.0"
  }
}
